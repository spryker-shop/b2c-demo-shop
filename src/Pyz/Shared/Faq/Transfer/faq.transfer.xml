<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Faq">
        <property name="idFaq" type="int"/>
        <property name="question" type="string"/>
        <property name="answer" type="string"/>
        <property name="enabled" type="bool"/>
        <property name="translations" type="LocalizedFaq[]"/>
    </transfer>

    <transfer name="FaqLocalized">
        <property name="idFaqLocalized" type="int"/>
        <property name="question" type="string"/>
        <property name="answer" type="string"/>
        <property name="storeRelation" type="string" />
        <property name="idFaq" type="int" />
        <property name="originalFaq" type="Faq" />
    </transfer>

</transfers>
