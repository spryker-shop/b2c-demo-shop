{% extends molecule('agent-navigation', '@SprykerShop:AgentPage') %}
{% import _self as component %}

{% block body %}
    <ul class="{{ component.renderClass('menu', modifiers) }}">
        <li class="{{ component.renderClass('menu__item', modifiers) }} {{ component.isActive('overview', data.activePage) }}">
            <a class="{{ component.renderClass('menu__link', modifiers) }}" href="{{ path('agent/overview') }}">
                {{ 'agent.account.overview' | trans }}
            </a>
        </li>
        {% if widgetGlobalExists('SetMultiFactorAuthMenuItemWidget') %}
            {% widget 'SetMultiFactorAuthMenuItemWidget' args [data.activePage] only %}{% endwidget %}
        {% endif %}
    </ul>
{% endblock %}
