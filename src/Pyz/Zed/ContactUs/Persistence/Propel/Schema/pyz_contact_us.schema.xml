<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\ContactUs\Persistence"
          package="src.Orm.Zed.ContactUs.Persistence">

    <table name="pyz_contact_us" idMethod="native">
        <column name="id_contact_us" required="true" type="INTEGER" autoIncrement="true" primaryKey="true" />
        <column name="name" required="true" size="255" type="VARCHAR" />
        <column name="message" required="true" size="255" type="VARCHAR" />
    </table>

</database>
